{
  "env": {
    "browser": true,
    "node": false
  },
  "extends": [
    /** @see https://eslint.vuejs.org/rules/ */
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "react/react-in-jsx-scope": "off"
  }
}
